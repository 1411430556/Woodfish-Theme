{
  "name": "Woodfish Light",
  "type": "light",
  "colors": {
    "editor.background": "#ffffff",
    "editor.foreground": "#333333",
    "activityBar.background": "#f3f3f3",
    "activityBar.foreground": "#333333",
    "activityBarBadge.background": "#007acc",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#c4c4c4",
    "badge.foreground": "#333333",
    "button.background": "#007acc",
    "button.foreground": "#ffffff",
    "button.hoverBackground": "#005a9e",
    "dropdown.background": "#ffffff",
    "dropdown.foreground": "#333333",
    "editor.lineHighlightBackground": "#f7f7f7",
    "editor.selectionBackground": "#add6ff",
    "editor.selectionHighlightBackground": "#e8f4fd",
    "editorCursor.foreground": "#000000",
    "editorGroup.border": "#e7e7e7",
    "editorGroupHeader.tabsBackground": "#f3f3f3",
    "editorIndentGuide.background": "#d3d3d3",
    "editorLineNumber.foreground": "#237893",
    "editorWhitespace.foreground": "#b3b3b3",
    "editorWidget.background": "#f3f3f3",
    "input.background": "#ffffff",
    "input.foreground": "#333333",
    "list.activeSelectionBackground": "#0060c0",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#f0f0f0",
    "panel.background": "#ffffff",
    "panel.border": "#80808080",
    "panelTitle.activeForeground": "#424242",
    "peekViewEditor.background": "#f2f8fc",
    "scrollbarSlider.background": "#79797979",
    "scrollbarSlider.hoverBackground": "#646464b3",
    "sideBar.background": "#f3f3f3",
    "sideBar.foreground": "#333333",
    "sideBarSectionHeader.background": "#80808033",
    "statusBar.background": "#007acc",
    "statusBar.foreground": "#ffffff",
    "statusBar.noFolderBackground": "#68217a",
    "tab.activeBackground": "#ffffff",
    "tab.activeForeground": "#333333",
    "tab.border": "#f3f3f3",
    "tab.inactiveBackground": "#ececec",
    "tab.inactiveForeground": "#33333380",
    "titleBar.activeBackground": "#dddddd",
    "titleBar.activeForeground": "#333333",
    "titleBar.inactiveBackground": "#dddddd99",
    "titleBar.inactiveForeground": "#33333399"
  },
  "tokenColors": [
    {
      "name": "注释",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#008000"
      }
    },
    {
      "name": "变量",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#001080"
      }
    },
    {
      "name": "关键字",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#0000ff"
      }
    },
    {
      "name": "字符串",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key"
      ],
      "settings": {
        "foreground": "#a31515"
      }
    },
    {
      "name": "函数",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#795e26"
      }
    },
    {
      "name": "数字",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape"
      ],
      "settings": {
        "foreground": "#098658"
      }
    },
    {
      "name": "类型",
      "scope": [
        "entity.name",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#267f99"
      }
    }
  ]
}